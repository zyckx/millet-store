
.section-menu {
	margin: 47rpx 0;
}
.section .title {
	padding: 35rpx 0;
}
.select .list {
	padding: 0 22rpx;
}
.select .item {
	border: 1rpx solid #eee;
	margin-bottom: 30rpx;
}

/* 本家LOOK */
.look .list {
	background-color: #f8f8f8;
	padding: 21rpx;
}
.look .list .item {
	padding: 20rpx;
	background-color: white;
	margin-bottom: 20rpx;
}
.look .list .item:last-child {
	margin-bottom: 0;
}
.look .list .item .album {
	display: flex;
	justify-content: space-between;
	align-items: center;
}
.look .list .album .top {
	margin-bottom: 10rpx;
}

